{
  "name": "devmateai",
  "displayName": "DevMate AI",
  "description": "This extension helps you to optimize your code with AI and suggest you the best possible code, while calculating the time and space complexity with secure local LLM",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "optimizationwithai.apiResponseView",
          "name": "DEVMATE AI"
        }
      ]
    },
    "commands": [
      {
        "command": "optimizationwithai.hitApi",
        "title": "Hit API"
      },
      {
        "command": "optimizationwithai.showCodeSuggestion",
        "title": "Show Code Suggestion"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@babel/parser": "^7.25.4",
    "@babel/traverse": "^7.25.4",
    "acorn": "^8.12.1",
    "acorn-jsx": "^5.3.2",
    "acorn-walk": "^8.3.3",
    "acron": "^1.0.5",
    "axios": "^1.7.5",
    "highlight.js": "^11.10.0"
  }
}
